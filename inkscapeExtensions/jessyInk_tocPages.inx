				    <_option value="none">None</_option>
						<_option value="color">Color</_option>
					  <_option value="alpha">Alpha</_option>
				    <_option value="both">Both</_option>
				  </param>
			    <param name="subsection_ns_rgba" type="color">#000000</param>
        </page>
				<page name="subsection_pss" _gui-text="Previous subsections">
					<param name="subsection_pss_style" type="optiongroup"  appearance="minimal" _gui-text="Style">
				    <_option value="hidden">Hidden</_option>
				    <_option value="none">None</_option>
						<_option value="color">Color</_option>
					  <_option value="alpha">Alpha</_option>
				    <_option value="both">Both</_option>
